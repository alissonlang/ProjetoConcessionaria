                    alt="SUVs">
            </fieldset>
        </div>
        <div class="picapes">
            <fieldset>
                <legend>Picapes</legend>
                <img src="https://catalogo.webmotors.com.br/imagens/prod/fotos-temas/CardDesktop/2e948af3-f8a6-41e0-a8da-594248b7c4bc_CardDesktop.webp?s=fill&w=274&h=216&q=70"
                    alt="Picapes">
            </fieldset>
        </div>
        
        <div class="listagem">
            <% for(i=0; i<veiculos.length; i++){ %>
              <h3><%= veiculos[i].modelo %></h3>
              <p><%= veiculos[i].marca %></p>
              <a href="detalhes/<%= veiculos[i].id %>">Ver detalhes</a>
              <hr>
            <% } %>
          </div>
           
      
          <div class="carros">
              <img src="../images/img.png">
              <div>
                <h1>JEEP RENEGADE</h1>
                <h2>6.500</h2>
                <span>R$ 149.990</span>
                <button>Saiba mais</button>
              </div>
            </div>
          
            <div class="carros">
              <img src="..images/images.png">
              <div>
                <h1>CHEVROLET ONIX</h1>
                <h2>61.083</h2>
                <span>R$ 66.800</span>
                <button>Saiba mais</button>
              </div>
            </div>
          
            <div class="carros">
              <img src="..images/pngwing.com.png">
              <div>
                <h1>TOYOTA HILUX</h1>
                <h2>22.990</h2>
                <span>R$ 275.900</span>
